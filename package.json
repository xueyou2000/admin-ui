{
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-layout": "^6.8.0",
    "@ant-design/pro-table": "^2.16.4",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-react": "^1.6.10",
    "@umijs/test": "^3.2.23",
    "antd": "^4.7.0",
    "classnames": "^2.2.6",
    "crypto-js": "^4.0.0",
    "dva": "^2.4.1",
    "lint-staged": "^10.0.7",
    "omit.js": "^2.0.2",
    "prettier": "^1.19.1",
    "qs": "^6.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.7",
    "react-router": "^5.2.0",
    "umi": "^3.3.3",
    "umi-plugin-keep-alive": "^0.0.1-beta.16",
    "use-merge-value": "^1.0.2",
    "xy-css-transition": "^0.1.0",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/crypto-js": "^4.0.1",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "mockjs": "^1.1.0",
    "roadhog-api-doc": "^1.1.2"
  }
}
